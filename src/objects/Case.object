    <validationRules>
        <fullName>cases_Assign_to_Q_or_User</fullName>
        <active>false</active>
        <description>Only allow user to choose Assign To User OR Assign To Queue</description>
        <errorConditionFormula>CASE(IF(ISBLANK(TEXT(reassign_to_queue__c)), 0, 1) +IF(ISBLANK(TEXT(Reassign_to_User__c)), 0, 1), 1, 1,0)=0</errorConditionFormula>
        <errorMessage>You may only Assign to a Queue OR Assign to a User, not both</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>closed_Cases</fullName>
        <active>true</active>
        <description>moves closed cases from Department Queues</description>
        <errorConditionFormula>AND(
         NOT(ISBLANK(Owner:Queue.Id)),
         ISPICKVAL(Status, &quot;Closed&quot;)
)</errorConditionFormula>
        <errorMessage>failed, use assignment rule checkbox at the bottom of the page</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>update_request</fullName>
        <availability>online</availability>
        <description>Updates Monetary Request Status in Backoffice</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Update Request Status</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var backofficeUrl = &apos;https://ib-backoffice.stox.com&apos;;

function loadScript(url, callback, errCallback){
    var script = document.createElement(&quot;script&quot;)
    script.type = &quot;text/javascript&quot;;

    if (script.readyState){  //IE
        script.onreadystatechange = function(){
            if (script.readyState == &quot;loaded&quot; ||
                script.readyState == &quot;complete&quot;){
                script.onreadystatechange = null;
                callback();
            }
        };
    } else {  //Others
        script.onload = function(){
            callback();
        };
        script.onerror = function(err) {
            errCallback(err);
        };
    }
    script.src = url;
    document.getElementsByTagName(&quot;head&quot;)[0].appendChild(script);
}


loadScript(backofficeUrl + &apos;/salesforce/scripts/init&apos;, function(){       
     var backofficeApi = stoxBackoffice(backofficeUrl);

     var request = {
        id: &apos;{!Case.request_id__c}&apos;,
        type: &apos;{!Case.Reason}&apos;,
        amount: &apos;{!Case.amount__c}&apos;,
        currency: &apos;{!Case.currency__c}&apos;,
        status: &apos;{!Case.Status}&apos;,
        userId: &apos;{!Account.stox_id__c}&apos;,
        username: &apos;{!Account.username__c}&apos;,
        currentStatus: &apos;{!Case.Status}&apos;,
        timestamp: &apos;{!Case.CreatedDate}&apos;
    };        
        backofficeApi.updateRequest(request);
    }, function(err) {
       alert(&apos;Connection to backoffice failed&apos;);
       console.log(err);
    });</url>
    </webLinks>
