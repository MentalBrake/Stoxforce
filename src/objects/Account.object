        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Update Client</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var http = new XMLHttpRequest();

var url = &apos;https://ib-backoffice-server.stox.com/salesforce/update_account_state/&apos;;
var params = &quot;account_id=&quot; + &quot;{!Account.x18_digits_Id__c}&quot; + &quot;&amp;session=&quot; + &quot;{!$Api.Session_ID}&quot; + &quot;&amp;instance=&quot; + document.domain + &quot;&amp;user_id=&quot; + &quot;{!$User.Id}&quot; + &quot;&amp;deposited=&quot; + &quot;{!Account.deposited__c}&quot; + &quot;&amp;initial_deposit_amount=&quot; + &quot;{!Account.initial_deposit_amount__c}&quot; + &quot;&amp;initial_deposit_currency=&quot; + &quot;{!Account.initial_deposit_currency__c}&quot; + &quot;&amp;passport_status=&quot; + &quot;{!Account.passport_status__c}&quot; + &quot;&amp;passport_reject_reason=&quot; + &quot;{!Account.passport_reject_reason__c}&quot; + &quot;&amp;utility_bill_status=&quot; + &quot;{!Account.utility_bill_status__c}&quot; + &quot;&amp;utility_bill_reject_reason=&quot; + &quot;{!Account.utility_bill_reject_reason__c}&quot;;

http.open(&quot;POST&quot;, url, true);
http.setRequestHeader(&quot;Content-type&quot;, &quot;application/x-www-form-urlencoded&quot;);

        http.onreadystatechange = function () {
            if (http.readyState == 4) {
                if (http.status == 200) {
                    alert(&apos;Successfuly updated client&apos;);
                }
                else {
                    alert(&quot;Error: &quot; + http.status);
                }
            }
        };
        http.send(params)</url>
